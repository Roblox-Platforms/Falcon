{
	"name": "@roplatforms/falcon",
	"globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
	"tree": {
		"$path": "build",
		"LICENSE": {
			"$path": "LICENSE.luau"
		},
		"include": {
			"$path": "include",
			"node_modules": {
				"$className": "Folder",
				"@rbxts": {
					"$path": "node_modules/@rbxts"
				}
			}
		}
	}
}

